<%- include('include/_header')%>


<main id="site-main">
  <div class="container">
      <div class="box-nav d-flex justify-between text-left ">
          <a href="/add_contact" class="border-shadow-top">
            <span class="text-gradient left-side">
                <i class="uil uil-envelope"></i> New Contact
            </span>
            </a>
      </div>

      <form action="/" method="POST">
        <table class="table">
          <thead class="thread-dark">
            <tr>
              <th>ID</th>
              <th>Full Name</th>
              <th>Gender</th>
              <th>Email</th>
              <th>Phone</th>
              <th>Company</th>
              <th>Birthday</th>
              <th>Action</th>
            </tr>
          </thead>
          <tbody>
            <%- include('include/_show')%>
          </tbody>
        </table>
      </form>
  </div>
</main>

<%- include('include/_footer')%>