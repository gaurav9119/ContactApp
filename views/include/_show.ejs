<% for(var i = 0; i < users.length; i++) { %>
<tr>
  <td><%= i + 1 %></td>
  <td><%= users[i].name %></td>
  <td><%= users[i].email %></td>
  <td><%= users[i].gender %></td>
  <td><%= users[i].phone %></td>
  <td><%= users[i].company %></td>
  <td><%= users[i].birthday %></td>
    <td>
      <a href="update_contact?id=<%= users[i]._id%>" class="btn border-shadow update">
        <span class="text-gradient">
          <i class="uil uil-edit-alt"></i>
        </span>
      </a>
      <a href="update_contact" class="btn border-shadow delete" data-id=<%= users[i]._id%> >
        <span class="text-gradient">
          <i class="uil uil-trash-alt"></i>
        </span>
      </a>
    </td>
  </tr>
  <% } %>